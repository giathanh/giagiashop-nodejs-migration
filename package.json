{
  "name": "giagiashop-nodejs-api",
  "version": "0.0.1",
  "description": "api for nodejs giagiashop",
  "main": "index.js",
  "engines": {
    "node": "12.16.1",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run prettier && npm run tslint && npm audit",
    "clean": "rimraf ./dst ./docs ./coverage \"npm-debug.log*\" ./.nyc_output",
    "start": "node dst/server.js",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test && npm run doc",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giathanh/project-management-node-api.git"
  },
  "author": "thanh.clg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/giathanh/project-management-node-api/issues"
  },
  "homepage": "https://github.com/giathanh/project-management-node-api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chardet": "^1.3.0",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.2.0",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "qs": "^6.7.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.14.165",
    "@types/multer": "^1.4.4",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/qs": "^6.9.5",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  }
}
